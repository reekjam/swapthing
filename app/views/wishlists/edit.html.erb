<div id='wishlist'>
  <h2>My Wishlist</h2>
  <p class='hint'>View, edit, or add to your wishlist items</p>

  <%= react_component('EditWishlistItems', 
    props: {
      authenticityToken: form_authenticity_token,
      items: @wishlist_items,
      isOwner: params[:user_id].present?,
      wishlistId: @wishlist.id,
      userId: current_user.id
    }
  ) %>
</div>
